{
  "name": "famifarm-management",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "add": "^2.0.6",
    "install": "^0.12.1",
    "keycloak-js": "^4.5.0",
    "localized-strings": "0.0.11",
    "lodash": "^4.17.11",
    "moment-range": "^4.0.2",
    "node-sass": "^4.14.0",
    "node-sass-chokidar": "^1.3.3",
    "noty": "^3.2.0-beta",
    "npm": "^6.4.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "^16.4.2",
    "react-infinite-scroller": "^1.2.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-select": "^2.0.0",
    "react-semantic-ui-datepickers": "^1.8.1",
    "react-vertical-timeline-component": "^2.3.1",
    "redux": "^4.0.0",
    "semantic-ui-calendar-react": "^0.11.0",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.3",
    "urijs": "^1.19.10"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-ts": "react-scripts-ts start",
    "build-ts": "react-scripts-ts build",
    "start": "npm-run-all -p watch-css start-ts",
    "build": "npm-run-all build-css build-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-client": "./node_modules/.bin/openapi-generator-cli generate -i swagger.yaml -o ./src/generated/client -c generator-config.json -g typescript-fetch"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.18",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-infinite-scroller": "^1.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-select": "^2.0.4",
    "@types/react-vertical-timeline-component": "^2.3.1",
    "@types/urijs": "^1.15.38",
    "metatavu-react-tsrules": "0.0.1",
    "typescript": "^3.0.1"
  }
}
